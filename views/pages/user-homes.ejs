                  <input type="text" class="form-control" id="macId" name="macId" disabled>
                  <div class="error-message" data-error="macId"></div>
               </div>

               <div class="mb-3">
                  <label for="wifiId" class="form-label">Id của Wifi</label>
                  <input type="text" class="form-control" id="wifiId" name="wifiId" disabled>
                  <div class="error-message" data-error="wifiId"></div>
               </div>

               <div class="mb-3">
                  <label for="wifiPassword" class="form-label">Mật khẩu của Wifi</label>
                  <input type="text" class="form-control" id="wifiPassword" name="wifiPassword" disabled>
                  <div class="error-message" data-error="wifiPassword"></div>
               </div>
            </form>

         </div>
         <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Đóng</button>
         </div>
      </div>
   </div>
</div>
