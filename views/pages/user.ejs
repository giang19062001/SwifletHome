<div class="common-table-container row" id="page-user">
   <div class="col-xl-12">
      <div class="row mb-3">
         <div class="col-12">
            <div class="card">
               <div class="card-body">
                  <div class="d-flex justify-content-between">
                     <p class="title-page">
                        <%= title %>
                     </p>
                  </div>
               </div>
            </div>
         </div>
      </div>
      <div class="row">
         <div class="col-12">
            <div class="container-table">
               <div class="table-responsive">
                  <table class="table table-striped">
                     <thead class="table-dark">
                        <tr class="text-center">
                           <th>
                              <p>STT</p>
                           </th>
                           <th>
                              <p>Tên khách hàng</p>
                           </th>
                           <th>
                              <p>Số điện thoại</p>
                           </th>
                           <th>
                              <p>Gói</p>
                           </th>
                           <th>
                              <p>Ngày bắt đầu gói</p>
                           </th>
                           <th>
                              <p>Ngày kết thúc gói</p>
                           </th>
                           <th>
                              <p>Số ngày còn lại</p>
                           </th>
                           <th>
                              <p>Ngày đăng ký</p>
                           </th>
                           <th>

                           </th>
                        </tr>
                     </thead>
                     <tbody class="body-table"></tbody>
                  </table>
               </div>
               <nav>
                  <ul class="pagination" id="privacy-main-pager"></ul>
               </nav>
            </div>
         </div>
      </div>
   </div>
</div>
</div>
<!-- modal update-->
<div class="modal common-modal user-update-modal" tabindex="-1" role="dialog" aria-hidden="true">
   <div class="modal-dialog modal-md modal-dialog-centered" role="document">
      <div class="modal-content">
         <div class="modal-header">
            <p class="modal-header-title">Chỉnh sửa</p>
            <button type="button" class="close btn" data-bs-dismiss="modal" aria-label="Close">
               <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M4.5 4.5L19.5 19.5" stroke="#212529" stroke-width="3" stroke-linecap="round" />
                  <path d="M19.5 4.5L4.5 19.5" stroke="#212529" stroke-width="3" stroke-linecap="round" />
               </svg>
            </button>
         </div>
         <div class="modal-body">
            <form>
               <input type="hidden" id="userCode" value="">

               <div class="row mb-3">
                  <div class="col-md-6">
                     <label for="userName" class="form-label">Tên khách hàng</label>
                     <input class="form-control" id="userName" disabled></input>
                  </div>
                  <div class="col-md-6">
                     <label for="userPhone" class="form-label">Số điện thoại</label>
                     <input class="form-control" id="userPhone" disabled></input>
                  </div>
               </div>
               <div class="row mb-3">
                  <div class="col-md-12">
                     <label for="packageCode" class="form-label">Gói đang dùng</label>
                     <select class="form-select" id="packageCode"></select>
                  </div>

               </div>

            </form>
         </div>
         <div class="modal-footer">
            <button type="button" class="btn-submit">Lưu thông tin</button>
         </div>
      </div>
   </div>
</div>
<script src="/js/page-user.js"></script>