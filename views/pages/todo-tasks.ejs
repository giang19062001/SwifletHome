<div class="common-table-container row" id="page-todo-tasks">
   <div class="col-xl-12">
      <div class="row mb-3">
         <div class="col-12">
            <div class="card">
               <div class="card-body">
                  <div class="d-flex justify-content-between">
                     <p class="title-page">
                        <%= title %>
                     </p>
                  </div>
               </div>
            </div>
         </div>
      </div>


      <div class="row mb-3" id="box-tasks" style="display: none;">
         <div class="col-12">
            <div class="card p-3">

               <h5 class="card-title mb-4">Thiết lập hộp thoại</h5>

               <div class="d-flex gap-3 flex-wrap">
                  <div class="card card-box flex-fill shadow-sm rounded-3 p-3">
                     <span class="card-badge">Hộp thoại 1</span>
                     <div class="card-body text-center py-4">
                        <select class="form-select" id="box-1" data-seq="1" name="select-tasks">
                           <option value="">--Chọn công việc--</option>
                        </select>
                     </div>
                  </div>
                  <div class="card card-box flex-fill shadow-sm rounded-3 p-3">
                     <span class="card-badge">Hộp thoại 2</span>
                     <div class="card-body text-center py-4">
                        <select class="form-select" id="box-2" data-seq="2" name="select-tasks">
                           <option value="">--Chọn công việc--</option>
                        </select>
                     </div>
                  </div>
                  <div class="card card-box flex-fill shadow-sm rounded-3 p-3">
                     <span class="card-badge">Hộp thoại 3</span>
                     <div class="card-body text-center py-4">
                        <select class="form-select" id="box-3" data-seq="3" name="select-tasks">
                           <option value="">--Chọn công việc--</option>
                        </select>
                     </div>
                  </div>
               </div>

               <button type="button" class="btn-submit btn-sm d-block mx-auto mt-3" id="update-box-task-btn">
                  Cập nhập
               </button>


            </div>
         </div>
      </div>

      <div class="row">
         <div class="col-12">
            <div class="container-table">
               <div class="table-responsive">
                  <table class="table table-striped">
                     <thead class="table-dark">
                        <tr class="text-center">
                           <th>
                              <p>STT</p>
                           </th>
                           <th>
                              <p>Tên công việc</p>
                           </th>
                           <th>
                              <p>Ngày tạo</p>
                           </th>
                           <th>
                              <p>Người tạo</p>
                           </th>
                        </tr>
                     </thead>
                     <tbody class="body-table"></tbody>
                  </table>
               </div>
               <nav>
                  <ul class="pagination" id="privacy-main-pager"></ul>
               </nav>
            </div>
         </div>
      </div>
   </div>
</div>
</div>
<script src="/js/page-todo-tasks.js"></script>